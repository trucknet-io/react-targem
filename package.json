{
  "name": "react-targem",
  "version": "0.0.0",
  "description": "A React library for efficiently implementing Gettext localization",
  "main": "./dist/bundles/index.umd.js",
  "module": "./dist/esm5/index.js",
  "es2015": "./dist/esm2015/index.js",
  "typings": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trucknet-io/react-targem.git"
  },
  "keywords": [
    "react",
    "targem",
    "i18n",
    "typescript",
    "localization"
  ],
  "author": "goooseman, alexeychikk",
  "license": "MIT",
  "scripts": {
    "pretest": "npm run import-strings",
    "prestart": "npm run import-strings",
    "prebuild": "npm run import-strings",
    "prelint": "npm run import-strings",
    "precommit": "npm run export-strings && git add dev/src/i18n/*.po* && lint-staged && npm run lint",
    "export-strings": "gettext-utils export-strings 'dev/src/**/{*.ts,*.tsx}' dev/src/i18n/template.pot --default-locale=en-GB",
    "import-strings": "gettext-utils import-strings dev/src/i18n dev/src/i18n/translations.json",
    "commit": "commit-prompt --no-verify",
    "lint": "npm run lint:tsc && npm run lint:tslint",
    "lint:tsc": "tsc -p dev/tsconfig.json --noEmit",
    "lint:prettier": "prettier --write \"{src,dev/src,test}/**/*.ts*\"",
    "lint:tslint": "tslint -c tslint.json -p ./dev/tsconfig.json --fix",
    "start": "env-cmd development webpack-dev-server --config dev/webpack.config.js --hot",
    "build": "rimraf 'dist' && npm run build:all",
    "build:all": "tsc && tsc --target es2018 --outDir dist/esm2015 && rollup -c config/rollup.config.js && rollup -c config/rollup.config.js --environment NODE_ENV:production",
    "postbuild": "npm run size",
    "size": "node config/file-size ./dist/bundles/index.umd.min.js ./dist/bundles/index.esm.min.js",
    "serve": "ws --directory dev/www --port 3000 --spa index.html",
    "clean": "rimraf '{src,dev/src,__tests__}/**/*.js?(x).map' 'node_modules/.cache' '.awcache'",
    "prerelease": "gettext-utils validate-strings",
    "release": "./config/deployment/release.sh",
    "test": "jest --detectOpenHandles",
    "test:ci": "npm run test -- --coverage"
  },
  "dependencies": {
    "lodash.get": "^4.4.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/lodash.get": "^4.4.6",
    "@types/node": "^12.7.1",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "brotli-size": "0.0.3",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.5.3",
    "env-cmd": "^8.0.2",
    "file-loader": "^3.0.1",
    "gettext-utils": "^1.2.3",
    "gzip-size": "5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.0",
    "jest-date-mock": "^1.0.7",
    "jest-dom": "^3.1.3",
    "kleur": "3.0.1",
    "markdown-loader": "^5.0.0",
    "prettier-config-trucknet-io": "^2.0.1",
    "pretty-bytes": "5.1.0",
    "raw-loader": "^3.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.8.4",
    "react-testing-library": "~6.1.0",
    "rimraf": "^2.6.3",
    "rollup": "0.68.2",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-json": "3.1.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-terser": "3.0.0",
    "rollup-plugin-uglify": "6.0.0",
    "start-server-and-test": "^1.7.11",
    "trucknet-commit": "^1.4.3",
    "ts-jest": "^24.0.0",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.18.0",
    "tslint-config-trucknet-io": "^1.8.2",
    "typescript": "^3.4.4",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-config-utils": "^2.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpackbar": "^3.2.0"
  },
  "peerDependencies": {
    "react": "^16.3.0"
  }
}
