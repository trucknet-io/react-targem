{
  "compilerOptions": {
    "importHelpers": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react",
    "lib": ["es2015", "es2017", "dom"],
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "target": "es5",
    "module": "esnext"
  }
}
